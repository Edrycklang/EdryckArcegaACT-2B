

.profile img {
    border: 4px solid #90e0ef;
    border-radius: 12px; 
    width: 150px;
    aspect-ratio: 1 / 1; 
    object-fit: cover;  
    display: block;
    margin: 0 auto;   
}


section ul {
    list-style-type: none;
    padding-left: 0;
}

section li {
    margin-bottom: 10px;
    line-height: 1.5;
    position: relative;
    padding-left: 20px;
}

